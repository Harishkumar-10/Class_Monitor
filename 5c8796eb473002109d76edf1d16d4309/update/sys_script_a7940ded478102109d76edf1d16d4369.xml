        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    // Add your code here
    var message = "Subject: " + current.subject + " by " + current.getDisplayValue('subject_teacher');
    var schDate = current.scheduled_date;
    var startDate;
    var endDate;
    var ses = current.session;
    if (ses == 1) {
        startDate = schDate + " 09:00:00";
        endDate = schDate + " 11:00:00";
    } else if (ses == 2) {
        startDate = schDate + " 11:00:00";
        endDate = schDate + " 13:00:00";

    } else if (ses == 3) {
        startDate = schDate + " 14:00:00";
        endDate = schDate + " 16:00:00";

    } else if (ses == 4) {
        startDate = schDate + " 16:00:00";
        endDate = schDate + " 18:00:00";

    }
    var gsdt = new GlideDateTime(startDate);
    var gedt = new GlideDateTime(endDate);

    current.setValue('class_start_date', gsdt);
    current.setValue('class_end_date', gedt);
    current.setValue('title', message);





})(current, previous);]]></script>
